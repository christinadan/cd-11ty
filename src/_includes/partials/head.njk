<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta name="author" content="{{ site.author.name }}">
  <meta name="description" content="{{ site.description }}">
  <link rel="icon" type="image/png" href="{{ site.favicon.large }}" sizes="32x32" />
  <link rel="icon" type="image/png" href="{{ site.favicon.small }}" sizes="16x16" />
  {% set css %}
    {% include "global/css/main.css" %}
  {% endset %}
  <style type="text/css">
    {{ css | cssmin | safe }}
  </style>
  <title>
    {% if title %}
      {{ title + " - " if title }}
    {% endif %}
    Christina Dan
  </title>
  <link href="https://fonts.googleapis.com/css2?family=Inter:wght@300;400;500;600;700&display=swap" rel="stylesheet">
  {% set js %}
    {% include "global/js/inline.js" %}
  {% endset %}
  <script>
    {{ js | jsmin | safe }}
  </script>
</head>