<ul class="nav-list social-links flex-row">
  {% set navItems = navigation.social %}
  {% for item in navItems %}
    <li class="nav-item">
      {% set rel = '' %}
      {% if item.external == true %}
        {% set rel = ' rel="noopener noreferrer me"' %}
      {% endif %}
      <a href="{{ item.url }}" aria-label="{{ item.text }}" {{ rel | safe }}>
        {% include item.icon %}
      </a>
    </li>
  {% endfor %}
</ul>