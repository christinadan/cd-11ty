<ul class="nav-list">
  {% set navItems = navigation.social %}
  {% for item in navItems %}
    {% if item.external %}
      {% set externalAttribute = ' rel="external"' %}
    {% endif %}

    <li class="nav-item">
      <a href="{{ item.url }}" {{ externalAttribute | safe }} target="_blank">
        {{ item.text }}
      </a>
    </li>
  {% endfor %}
</ul>